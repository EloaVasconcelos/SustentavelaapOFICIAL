<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/tela6"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity2sexta">

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="505dp"
        android:layout_height="34dp"
        android:background="#5CEC62"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="-29dp"
        tools:layout_editor_absoluteY="809dp"
        tools:ignore="DuplicateIds,MissingConstraints,VisualLintBounds">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="505dp"
        android:layout_height="34dp"
        android:background="#5CEC62"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="-2dp"
        tools:layout_editor_absoluteY="-4dp"
        tools:ignore="DuplicateIds,MissingConstraints,VisualLintBounds">

    </LinearLayout>

    <Button
        android:id="@+id/btnponto1"
        android:layout_width="248dp"
        android:layout_height="55dp"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="4dp"
        android:backgroundTint="#00bf63"
        android:fontFamily="sans-serif-black"
        android:text="Pontos de Coletas "
        android:textSize="22sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout"
        tools:ignore="HardcodedText,TextContrastCheck,TextSizeCheck" />

    <EditText
        android:id="@+id/txtlocal"
        android:layout_width="382dp"
        android:layout_height="63dp"
        android:ems="10"
        android:fontFamily="sans-serif-condensed"
        android:inputType="textPostalAddress"
        android:text="Digite sua Localização"
        android:textSize="32dp"
        tools:ignore="Autofill,HardcodedText,LabelFor,MissingConstraints,SpUsage,VisualLintBounds"
        tools:layout_editor_absoluteX="13dp"
        tools:layout_editor_absoluteY="206dp" />

    <Button
        android:id="@+id/btnverificar2"
        android:layout_width="258dp"
        android:layout_height="61sp"
        android:backgroundTint="#00bf63"
        android:fontFamily="sans-serif-black"
        android:text="VERIFICAR"
        android:textSize="34sp" tools:ignore="HardcodedText,MissingConstraints,TextSizeCheck,TextContrastCheck"
        tools:layout_editor_absoluteX="76dp"
        tools:layout_editor_absoluteY="315dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="377dp"
        android:layout_height="224dp"
        app:srcCompat="@drawable/map"
       android:contentDescription="map"
        tools:layout_editor_absoluteX="18dp"
        tools:layout_editor_absoluteY="428dp" />

    <Button
        android:id="@+id/btnvoltar3"
        android:layout_width="158dp"
        android:layout_height="65dp"
        android:backgroundTint="#00bf63"
        android:text="Voltar"
        android:textSize="24sp" tools:ignore="HardcodedText,MissingConstraints,TextContrastCheck,VisualLintBounds"
        tools:layout_editor_absoluteX="157dp"
        tools:layout_editor_absoluteY="737dp" />

</androidx.constraintlayout.widget.ConstraintLayout>